@model System.Data.DataTable
@{
    ViewBag.Title = "Index";
}

<hr />
<h2 class="text-primary">ALL Projects</h2>
<hr />
<table class="table table-bordered table-striped">
    <tr>
        <th>Project Name</th>
        <th>Start Date </th>
        <th>End Date</th>
        <th></th>
    </tr>

    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td>
                <a href="@Url.Action("Edit", "Project", new {@id=@Model.Rows[i][0] })">Edit</a>
                <a href="@Url.Action("Delete", "Project", new {@id=@Model.Rows[i][0] })">Delete</a>
            </td>
        </tr>
    }
</table>

<a href="@Url.Action("Create","Project")">Add Project</a>